<%- include('partial/header.ejs')%>
  <!--? Contact form Start -->
  <section class="contact-form-main">
    <div class="container">
        <div class="row justify-content-end">
            <div class="col-xl-7 col-lg-7">
                <div class="form-wrapper">
                    <!--Section Tittle  -->
                    <div class="form-tittle">
                        <div class="row ">
                            <div class="col-lg-11 col-md-10 col-sm-10">
                                <div class="section-tittle">
                                    <span>Formulaire de contact</span>
                                    <h2>laisser nous des commentaires et suggestions!</h2>
                                </div>
                            </div>
                        </div>
                    </div>
                    <!--End Section Tittle  -->
                    <form id="contact-form" action="#" method="POST">
                        <div class="row">
                            <div class="col-lg-6 col-md-6">
                                <h1> <label for="">name</label></h1>
                                <div class="form-box user-icon mb-30">
                                    <input type="text" name="name">
                                </div>
                            </div>
                            <div class="col-lg-6 col-md-6">
                                <h1> <label for="">phone</label></h1>
                                <div class="form-box email-icon mb-30">
                                    <input type="text" name="phone">
                                </div>
                            </div>

                            <div class="col-lg-6 col-md-6">
                                <h1> <label for="">email</label></h1>
                                <div class="form-box subject-icon mb-30">
                                    <input type="Email" name="subject">
                                </div>
                            </div>
                            <div class="col-lg-12">
                                <h1> <label for="">message</label></h1>
                                <div class="form-box message-icon mb-65">
                                    <textarea name="message" id="message"></textarea>
                                </div>
                                <div class="submit-info">
                                   <button class="btn" type="submit">Envoyez message</button> 
                                </div>
                            </div>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
    <!-- contact left Img-->
    <div style="margin-top: 90px;" class="from-left d-none d-lg-block">
        <img src="../assets/img/gallery/houblon_vert.jpg" alt="houblon vert">
    </div>
</section>
<!-- Contact form End -->

