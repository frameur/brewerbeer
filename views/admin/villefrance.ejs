        <div id="page-wrapper">
            <div class="container-fluid">
                <div class="row bg-title">
                    <div class="col-lg-3 col-md-4 col-sm-4 col-xs-12">
                        <h4 class="page-title"><%= title %> </h4>
                    </div>
                    <div class="col-lg-9 col-sm-8 col-md-8 col-xs-12">

                        <ol class="breadcrumb">
                            <li><a href="#">Dashboard</a></li>
                            <li class="active">ville</li>
                        </ol>
                    </div>
                    <!-- /.col-lg-12 -->
                </div>
                <div class="row">
                    <div class="col-lg-4 col-sm-6 col-xs-12">
                        <div class="white-box analytics-info">
                            <h3 class="box-title">Total villes de france</h3>
                            <ul class="list-inline two-part">
                                <li>
                                    <div id="sparklinedash"></div>
                                </li>
                                <li class="text-right"><i class="ti-arrow-up text-success"></i> <span class="counter text-success"><%= totalVilledefrance %></span></li>
                            </ul>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-12 col-lg-12 col-sm-12">
                        <div class="white-box">
                            <div class="col-md-3 col-sm-4 col-xs-6 pull-right">
                               
                            </div>
                            <h3 class="box-title">les villes de france</h3>
                            <div class="table-responsive">
                                <table class="table">
                                    <thead>
                                        <tr>
                                            <th>id</th>
                                            <th>ville departement</th>
                                            <th>ville slug</th>
                                            <th>nom de la ville</th>
                                            <th>nom simple</th>
                                            <th>nom reel</th>
                                            <th>nom soundex</th>
                                            <th>nom metaphone</th>
                                            <th>code postal</th>
                                            <th>commune</th>
                                            <th>code commune</th>
                                            <th>code arrondissement</th>
                                            <th>code canton</th>
                                            <th>longitude deg</th>
                                            <th>latitude deg</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <% villes_france_free.forEach((villesfrance, index) =>{ %>
                                        <tr>
                                            <td><%= villesfrance.ville_id %></td>
                                            <td><%= villesfrance.ville_departement %></td>
                                            <td><%= villesfrance.ville_slug %></td>
                                            <td><%= villesfrance.ville_nom %></td>
                                            <td><%= villesfrance.ville_nom_simple %></td>
                                            <td><%= villesfrance.ville_nom_reel %></td>
                                            <td><%= villesfrance.ville_nom_soundex%></td>
                                            <td><%= villesfrance.ville_nom_metaphone %></td>
                                            <td><%= villesfrance.ville_code_postal %></td>
                                            <td><%= villesfrance.ville_commune %></td>
                                            <td><%= villesfrance.ville_code_commune %></td>
                                            <td><%= villesfrance.ville_arrondissement %></td>
                                            <td><%= villesfrance.ville_canton %></td>
                                            <td><%= villesfrance.ville_longitude_deg %></td>
                                            <td><%= villesfrance.ville_latitude_deg %></td>
                                            
                                        </tr>
                                        <% }) %>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <%- include('../partial/admin/footer.ejs') %>