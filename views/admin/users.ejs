                <div class="row">
                    <div class="col-md-12 col-lg-12 col-sm-12">
                        <div class="white-box">
                           
                            <h3 class="box-title">membres</h3>
                            <div class="table-responsive">
                                <table class="table">
                                    <thead>
                                        <tr>
                                            <th>id</th>
                                            <th>Nom</th>
                                            <th>Prenom</th>
                                            <th>Email</th>
                                            <th>Age</th>
                                            <th>password</th>
                                            <th>Role</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <% users.forEach((user, index) =>{ %>
                                        <tr>
                                            <td><%= user.user_id %></td>
                                            <td><%= user.firstname %></td>
                                            <td><%= user.lastname %></td>
                                            <td><%= user.email %></td>
                                            <td><%= user.age %></td>
                                            <td><%= user.password %></td>
                                            <td><%= user.role_id %></td>
                                        </tr>
                                        <% }) %>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>

