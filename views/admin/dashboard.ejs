                                            <th>Email</th>
                                            <th>Age</th>
                                            <th>password</th>
                                            <th>Role</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <% users.forEach((user, index) =>{ %>
                                        <tr>
                                            <td><%= user.user_id %></td>
                                            <td><%= user.firstname %></td>
                                            <td><%= user.lastname %></td>
                                            <td><%= user.email %></td>
                                            <td><%= user.age %></td>
                                            <td><%= user.password %></td>
                                            <td><%= user.role_id %></td>
                                        </tr>
                                        <% }) %>
                                    </tbody>
                                </table>
                            </div>
                        </div>
