                            <h3 class="box-title">Utilisateurs</h3>
                            <div class="table-responsive">
                                <table class="table">
                                    <thead>
                                        <tr>
                                            <th>id</th>
                                            <th>Titre</th>
                                            <th>Contenu</th>
                                            <th>Auteur de l'article</th>
                                            <th>Date de l'article</th>
                                            <th>Date de publication</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <% actubeer.forEach((actu, index) => { %>
                                        <tr>
                                            <td><%= actu.actu_id %></td>
                                            <td><%= actu.actuTitle %></td>
                                            <td><%= actu.actuContent %></td>
                                            <td><%= actu.author %></td>
                                            <td><%= actu.Date %></td>
                                            <td><%= actu.created_at %></td>
                                        </tr>
                                        <% }) %>
                                    </tbody> 
                                </table>
                            </div>
                        </div>
                    </div>
