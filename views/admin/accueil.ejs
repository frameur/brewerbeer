    /* galerie */
    #galerie {
        text-align: center;
    }

    .container {
        width: 100%;
        min-height: 500px;
        margin: 70px auto 0;
        display: flex;
        flex-direction: row;
        flex-wrap: wrap;
        justify-content: center;



    }


    .container .box {
        position: relative;
        width: 260px;
        height: 170px;
        background: #ffff00;
        margin: 10px;
        box-sizing: border-box;
        display: inline-block;



    }

    .container .box .imgbox {
        position: relative;
        overflow: hidden;

    }

    .container .box .imgbox img {
        max-width: 100%;
        transition: transform 2s;


    }

    .container .box:hover .imgbox img {
        transform: scale(1.2);
    }

    .container .box .detail {
        position: absolute;
        top: 10px;
        left: 10px;
        bottom: 10px;
        right: 10px;
        background: rgb(0, 0, 0, .3);
        transform: scaleY(0);
        transition: transform .5s;
    }

    .container .box .detail .content {
        position: absolute;
        top: 50%;
        transform: translateY(-50%);
        text-align: center;
       
        color: #fff;

    }

    .container .box .detail .content h2 {
        margin: 0;
        padding: 0;
        font-size: 20px;
        color: #ffff00;
    }

    .container .box:hover .detail {
        transform: scaleY(1);


    }
</style>


